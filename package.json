{
  "name": "react-sigmajs",
  "description": "React SigmaJS - React.JS flow-typed interface for SigmaJS library - fastest opensource rendering engine for linked graphs.",
  "author": "Maxim Vorobjov <maxim.vorobjov@gmail.com>",
  "version": "0.3.0",
  "license": "MIT",
  "files": [
    "lib",
    "dist",
    "es",
    "sigma",
    "README.md",
    "LICENSE"
  ],
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "module": "es/index.js",
  "keywords": [
    "graph",
    "sigma.js",
    "force graph",
    "webgl graph",
    "neo4j graph",
    "json graph",
    "fast graph",
    "graph drawing"
  ],
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "autoprefixer": "6.5.1",
    "babel-cli": "^6.18.0",
