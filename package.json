    "object-assign": "4.1.0",
    "output-file-sync": "^1.1.2",
    "path-exists": "2.1.0",
    "promise": "7.1.1",
    "react-dev-utils": "^0.3.0",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4",
